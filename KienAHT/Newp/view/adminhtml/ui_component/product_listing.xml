<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="buttons" xsi:type="array">
            <item name="add" xsi:type="string">KienAHT\Newp\Block\Adminhtml\Newpro\Edit\BackButton</item>
        </item>
    </argument>
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="addnewproduct">
                <settings>
                    <confirm>
                        <message translate="true">Are you sure to add New Product to selected customers?
                            Please select Product is Configurable Product !!
                        </message>
                        <title translate="true">Add new Product</title>
                    </confirm>
                    <url path="newp/index/add"/>
                    <type>addnewproduct</type>
                    <label translate="true">Add new Product</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
